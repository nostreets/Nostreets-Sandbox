@using NostreetsExtensions.Extend.Basic;

<script type="text/javascript">
    (function () {
        "use strict";

        angular.module(page.APPNAME)
            .factory('$serverModel', baseServerModel);

        function baseServerModel() {

            var thisModel = this;

            thisModel = $.extend(true, {}, @(ViewBag.ServerModel != null ? @Html.Raw(((object)ViewBag.ServerModel).JsonSerialize()) : Html.Raw("{}")), thisModel);
            return thisModel;
        }

    })();
</script>

